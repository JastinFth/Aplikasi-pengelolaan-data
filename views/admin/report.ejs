<div class="mx-5">
  <div class="bg-custom-white shadow-sm rounded-2 p-5">
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Username</th>
          <th scope="col">Name</th>
          <th scope="col">Role</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach((user, no) => { %>
         <tr>
            <th scope="row"><%= no + 1 %></th>
            <td><%= user.username %></td>
            <td><%= user.name %></td>
            <td><%= user.role %></td>
         </tr>
        <% }) %>
      </tbody>
    </table>
    <div class="d-flex justify-content-end">
      <a href="/admin/report/download" class="btn btn-primary btn-taspen">
        <i class="fa-solid fa-file-arrow-down me-1"></i>
        Download Report
      </a>
    </div>
  </div>
</div>